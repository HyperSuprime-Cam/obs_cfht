        tables:        raw
        tables:        raw_skyTile
    }
    log: {
        template:      "logs/%(runId)s/%(object)s/%(date)s/%(filter)s/run.log"
        python:        "str"
        persistable:   "None"
        storage:       "TextStorage"
    }
    logDir: {
        template:      "logs/%(runId)s/%(object)s/%(date)s/%(filter)s/"
